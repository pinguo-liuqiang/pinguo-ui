{
  "name": "pinguo-web-ui",
  "version": "0.0.1",
  "main": "./dist/index.js",
  "module": "./dist/index.esm.js",
  "unpkg": "./dist/index.umd.js",
  "description": "品果科技-前端UI库",
  "author": "pinguo-web",
  "keywords": [
    "React",
    "components",
    "komens"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pinguo-liuqiang/pinguo-ui.git"
  },
  "bugs": {
    "url": "https://github.com/pinguo-liuqiang/pinguo-ui/issues"
  },
  "contributors": [
    "pinguo-liuqiang"
  ],
  "license": "MIT",
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "build:com": "father build",
    "dev": "father doc dev",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "^6.5.0",
    "@babel/runtime": "^7.14.0",
    "@umijs/preset-react": "1.x",
    "babel-plugin-import": "^1.13.3",
    "father": "^2.30.6",
    "umi": "^3.4.25"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@umijs/test": "^3.4.25",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "react": "17.x",
    "react-dom": "17.x",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  },
  "peerDependencies": {
    "react": "17.x"
  },
  "files": [
    "dist"
  ],
  "browserslist": [
    "last 2 version",
    "Firefox ESR",
    "> 1%",
    "ie >= 9"
  ]
}
